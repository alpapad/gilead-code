<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN" "http://java.sun.com/j2ee/dtds/web-app_2_2.dtd">
<web-app id="WebApp_ID">
	<display-name>CometChat</display-name>
	<servlet>
       <servlet-name>ChatServices</servlet-name>
       <servlet-class>net.sf.gilead.examples.server.ChatServicesImpl</servlet-class>
    </servlet>
    
    <servlet-mapping>
       <servlet-name>ChatServices</servlet-name>
       <url-pattern>net.sf.gilead.examples.CometChat/ChatServices</url-pattern>
    </servlet-mapping>
    
	
	<welcome-file-list>
        <welcome-file>CometChat.html</welcome-file>
        <welcome-file>index.html</welcome-file>
	</welcome-file-list>
	
   
</web-app>
