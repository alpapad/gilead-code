<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>SampleAppEngine.html</welcome-file>
  </welcome-file-list>
  
  <!-- Servlets -->
  <servlet>
    <servlet-name>userRemoteService</servlet-name>
    <servlet-class>net.sf.gilead.sample.server.UserRemoteService</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>userRemoteService</servlet-name>
    <url-pattern>/sampleappengine/UserRemote</url-pattern>
  </servlet-mapping>
  
   <!-- Servlets -->
  <servlet>
    <servlet-name>uploadServlet</servlet-name>
    <servlet-class>net.sf.gilead.sample.server.UploadFileServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>uploadServlet</servlet-name>
    <url-pattern>/sampleappengine/uploadHandler</url-pattern>
  </servlet-mapping>

</web-app>
